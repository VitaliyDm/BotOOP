<hibernate-mapping>
    <class name="TelegramBotUserSession" table="botoop.session">

        <id column="chatID" name="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>

        <property column="score" name="score" type="java.lang.Integer"/>
        <property column="userQuestions" name="userQuestions" type="java.lang.String"/>

    </class>
</hibernate-mapping>